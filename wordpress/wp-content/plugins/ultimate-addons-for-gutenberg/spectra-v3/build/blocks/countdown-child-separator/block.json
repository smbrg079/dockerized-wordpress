{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra/countdown-child-separator",
  "version": "3.0.0",
  "title": "Separator",
  "category": "spectra",
  "description": "Add a separator to the countdown.",
  "parent": [
    "spectra/countdown"
  ],
  "textdomain": "ultimate-addons-for-gutenberg",
  "usesContext": [
    "countdown",
    "spectra/countdown/showSeparator",
    "spectra/countdown/separatorType"
  ],
  "supports": {
    "lock": false,
    "html": false,
    "splitting": true,
    "shadow": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true,
      "__experimentalFontFamily": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalWritingMode": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontFamily": true
      }
    },
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "radius": true,
      "style": true,
      "shadow": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true,
        "radius": true,
        "style": true,
        "shadow": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "color": {
      "gradients": true,
      "link": false,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    }
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "text": {
      "type": "string"
    },
    "show": {
      "type": "boolean",
      "default": true
    },
    "textColorHover": {
      "type": "string"
    },
    "backgroundColorHover": {
      "type": "string"
    },
    "backgroundGradientHover": {
      "type": "string"
    }
  }
}