{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra/counter",
  "version": "3.0.0",
  "title": "Counter",
  "category": "spectra",
  "description": "Create animated numeric counters with customizable styles and layouts.",
  "textdomain": "ultimate-addons-for-gutenberg",
  "keywords": [
    "counter",
    "number",
    "count",
    "statistics",
    "stats",
    "metric",
    "figure",
    "data",
    "progress",
    "circular",
    "bar",
    "animated",
    "animation",
    "increment",
    "tally",
    "score",
    "total",
    "sum",
    "spectra",
    "uag",
    "uagb"
  ],
  "allowedBlocks": [
    "spectra/icons",
    "spectra/counter-child-number",
    "spectra/counter-child-progress-bar",
    "spectra/counter-child-wrapper",
    "spectra/content",
    "core/paragraph",
    "core/heading"
  ],
  "providesContext": {
    "spectra/counter/counterStyle": "counterStyle",
    "spectra/counter/startNumber": "startNumber",
    "spectra/counter/endNumber": "endNumber",
    "spectra/counter/totalNumber": "totalNumber",
    "spectra/counter/animationDuration": "animationDuration",
    "spectra/counter/prefix": "prefix",
    "spectra/counter/suffix": "suffix",
    "spectra/counter/thousandSeparator": "thousandSeparator",
    "spectra/counter/decimalPlaces": "decimalPlaces",
    "spectra/counter/progressColor": "progressColor",
    "spectra/counter/progressBackgroundColor": "progressBackgroundColor",
    "spectra/counter/progressSize": "progressSize",
    "spectra/counter/progressStrokeWidth": "progressStrokeWidth",
    "spectra/counter/barHeight": "barHeight",
    "spectra/counter/barBorderRadius": "barBorderRadius",
    "spectra/counter/prefixColor": "prefixColor",
    "spectra/counter/suffixColor": "suffixColor"
  },
  "supports": {
    "interactivity": true,
    "anchor": true,
    "align": [
      "wide",
      "full"
    ],
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "layout": {
      "default": {
        "type": "flex",
        "flexWrap": "nowrap",
        "orientation": "vertical",
        "justifyContent": "center",
        "verticalAlignment": "center"
      },
      "allowSizingOnChildren": true
    },
    "color": {
      "gradients": true,
      "link": false,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    }
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "viewScriptModule": "file:./view.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "layout": {
      "type": "object",
      "default": {
        "type": "flex",
        "flexWrap": "nowrap",
        "orientation": "vertical",
        "justifyContent": "center",
        "verticalAlignment": "center"
      },
      "allowSizingOnChildren": true
    },
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "anchor": {
      "type": "string"
    },
    "counterStyle": {
      "type": "string",
      "default": "simple"
    },
    "startNumber": {
      "type": "number",
      "default": 0
    },
    "endNumber": {
      "type": "number",
      "default": 100
    },
    "totalNumber": {
      "type": "number",
      "default": 100
    },
    "animationDuration": {
      "type": "number",
      "default": 2000
    },
    "prefix": {
      "type": "string",
      "default": ""
    },
    "suffix": {
      "type": "string",
      "default": ""
    },
    "thousandSeparator": {
      "type": "string",
      "default": ","
    },
    "decimalPlaces": {
      "type": "number",
      "default": 0
    },
    "progressColor": {
      "type": "string"
    },
    "progressBackgroundColor": {
      "type": "string"
    },
    "progressSize": {
      "type": "string"
    },
    "progressStrokeWidth": {
      "type": "number"
    },
    "barHeight": {
      "type": "string"
    },
    "barBorderRadius": {
      "type": "number"
    },
    "prefixRightMargin": {
      "type": "number",
      "default": 0
    },
    "suffixLeftMargin": {
      "type": "number",
      "default": 0
    },
    "prefixColor": {
      "type": "string"
    },
    "suffixColor": {
      "type": "string"
    },
    "progressBarPosition": {
      "type": "string",
      "default": "after"
    }
  }
}