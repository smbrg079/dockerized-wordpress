{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra/modal-popup-content",
  "version": "3.0.0",
  "title": "Modal-popup-content",
  "category": "spectra",
  "description": "This block allows you to add modal popup.",
  "textdomain": "ultimate-addons-for-gutenberg",
  "parent": [
    "spectra/modal-popup"
  ],
  "keywords": [
    "modal",
    "popup",
    "spectra",
    "uag",
    "uagb"
  ],
  "supports": {
    "interactivity": true,
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "radius": true,
      "style": true,
      "shadow": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true,
        "radius": true,
        "shadow": true
      }
    },
    "shadow": true,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalWritingMode": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontFamily": true
      }
    },
    "color": {
      "gradients": false,
      "background": false,
      "__experimentalDefaultControls": {
        "text": true
      }
    }
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": [
    "file:./style-index.css",
    "spectra-blocks-common",
    "spectra-components-block-appender"
  ],
  "usesContext": [
    "spectra/modal/modalPosition",
    "spectra/modal/hPos",
    "spectra/modal/vPos",
    "spectra/modal/closeIconPosition"
  ],
  "attributes": {
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "defaultTemplate": {
      "type": "boolean",
      "default": false
    },
    "icon": {
      "type": [
        "string",
        "object"
      ],
      "default": "xmark"
    },
    "iconPosition": {
      "type": "string"
    },
    "keyPress": {
      "type": "boolean",
      "default": true
    },
    "overlayClick": {
      "type": "boolean",
      "default": false
    },
    "containerWidth": {
      "type": "string"
    },
    "containerHeight": {
      "type": "string"
    },
    "maxContainerHeight": {
      "type": "string"
    },
    "contentHeight": {
      "type": "string"
    },
    "textColorHover": {
      "type": "string"
    },
    "backgroundColor": {
      "type": "string"
    },
    "backgroundColorHover": {
      "type": "string"
    },
    "backgroundGradient": {
      "type": "string"
    },
    "backgroundGradientHover": {
      "type": "string"
    },
    "background": {
      "type": "object"
    },
    "dimRatio": {
      "type": "number"
    }
  }
}