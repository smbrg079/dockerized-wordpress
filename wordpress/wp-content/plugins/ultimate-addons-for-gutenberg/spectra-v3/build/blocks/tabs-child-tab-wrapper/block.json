{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra/tabs-child-tab-wrapper",
  "version": "3.0.0",
  "title": "Tab Area",
  "category": "spectra",
  "description": "Add tabs to this tab block",
  "textdomain": "ultimate-addons-for-gutenberg",
  "allowedBlocks": [
    "spectra/tabs-child-tab-button"
  ],
  "parent": [
    "spectra/tabs"
  ],
  "supports": {
    "shadow": true,
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "radius": true,
      "style": true,
      "shadow": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true,
        "radius": true,
        "shadow": true
      }
    },
    "layout": {
      "default": {
        "type": "flex",
        "flexWrap": "nowrap",
        "justifyContent": "left",
        "verticalAlignment": "center"
      },
      "allowSizingOnChildren": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalWritingMode": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontFamily": true
      }
    },
    "color": {
      "gradients": true,
      "__experimentalDefaultControls": {
        "background": true
      }
    }
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "layout": {
      "type": "object",
      "default": {
        "type": "flex",
        "flexWrap": "nowrap",
        "justifyContent": "left",
        "verticalAlignment": "center"
      }
    },
    "isPreview": {
      "type": "boolean",
      "default": false
    }
  }
}