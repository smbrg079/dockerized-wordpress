{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra/countdown-child-label",
  "version": "3.0.0",
  "title": "Label",
  "category": "spectra",
  "description": "Add a block with text content.",
  "ancestor": [
    "spectra/countdown"
  ],
  "parent": [
    "spectra/countdown-child-day",
    "spectra/countdown-child-hour",
    "spectra/countdown-child-minute",
    "spectra/countdown-child-second",
    "spectra/container"
  ],
  "textdomain": "ultimate-addons-for-gutenberg",
  "usesContext": [
    "label",
    "showLabels",
    "spectra/countdown/showLabels"
  ],
  "supports": {
    "html": false,
    "splitting": true,
    "shadow": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true,
      "__experimentalFontFamily": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalWritingMode": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontFamily": true
      }
    },
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "radius": true,
      "style": true,
      "shadow": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true,
        "radius": true,
        "style": true,
        "shadow": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "color": {
      "gradients": true,
      "link": false,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    }
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "attributes": {
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "text": {
      "type": "string"
    },
    "textColorHover": {
      "type": "string"
    },
    "backgroundColorHover": {
      "type": "string"
    },
    "backgroundGradientHover": {
      "type": "string"
    }
  }
}