{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra/google-map",
  "version": "3.0.0",
  "title": "Google Map",
  "category": "spectra",
  "description": "Display an interactive Google Map with customizable features.",
  "textdomain": "ultimate-addons-for-gutenberg",
  "keywords": [
    "google map",
    "map",
    "location",
    "address",
    "embed map",
    "interactive map",
    "google maps",
    "mapping",
    "geolocation",
    "directions",
    "place marker",
    "embed",
    "add map",
    "create map",
    "insert map",
    "advanced map",
    "contact map",
    "store locator",
    "spectra",
    "uag",
    "uagb"
  ],
  "supports": {
    "anchor": true,
    "align": [
      "wide",
      "full"
    ],
    "shadow": true,
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "radius": true,
      "style": true,
      "shadow": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true,
        "radius": true,
        "shadow": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": true,
        "padding": true
      }
    }
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "address": {
      "type": "string",
      "default": "Brainstorm Force"
    },
    "enableSatelliteView": {
      "type": "boolean",
      "default": false
    },
    "height": {
      "type": "string"
    },
    "language": {
      "type": "string",
      "default": "en"
    },
    "zoom": {
      "type": "number",
      "default": 15
    },
    "mapTypeId": {
      "type": "string",
      "default": "roadmap"
    }
  }
}