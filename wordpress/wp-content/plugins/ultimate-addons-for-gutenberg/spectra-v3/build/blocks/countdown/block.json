{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra/countdown",
  "version": "3.0.0",
  "title": "Countdown",
  "category": "spectra",
  "description": "Create a sense of urgency among your visitors.",
  "textdomain": "ultimate-addons-for-gutenberg",
  "keywords": [
    "countdown",
    "timer",
    "countdown timer",
    "event countdown",
    "sale countdown",
    "launch countdown",
    "deadline timer",
    "time counter",
    "expiry timer",
    "urgency timer",
    "promotional countdown",
    "event timer",
    "clock",
    "sale",
    "offer",
    "discount",
    "deal",
    "spectra",
    "uag",
    "uagb"
  ],
  "providesContext": {
    "spectra/countdown/showDays": "showDays",
    "spectra/countdown/showHours": "showHours",
    "spectra/countdown/showMinutes": "showMinutes",
    "spectra/countdown/showSeconds": "showSeconds",
    "spectra/countdown/showLabels": "showLabels",
    "spectra/countdown/showSeparator": "showSeparator",
    "spectra/countdown/separatorType": "separatorType",
    "spectra/countdown/ariaLiveType": "ariaLiveType"
  },
  "supports": {
    "interactivity": true,
    "anchor": true,
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "radius": true,
      "style": true,
      "shadow": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true,
        "radius": true,
        "style": true,
        "shadow": true
      }
    },
    "layout": {
      "default": {
        "type": "flex",
        "flexWrap": "nowrap",
        "orientation": "horizontal",
        "justifyContent": "left",
        "verticalAlignment": "top"
      },
      "allowSizingOnChildren": true,
      "allowSwitching": true
    },
    "shadow": true,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalWritingMode": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontFamily": true
      }
    },
    "color": {
      "gradients": true,
      "link": false,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    }
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "viewScriptModule": "file:./view.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "anchor": {
      "type": "string"
    },
    "layout": {
      "type": "object",
      "default": {
        "type": "flex",
        "flexWrap": "nowrap",
        "orientation": "horizontal",
        "justifyContent": "left",
        "verticalAlignment": "top"
      }
    },
    "timerType": {
      "type": "string",
      "default": "date"
    },
    "endDateTime": {
      "type": "string"
    },
    "displayEndDateTime": {
      "type": "string"
    },
    "showLabels": {
      "type": "boolean",
      "default": true
    },
    "showDays": {
      "type": "boolean",
      "default": true
    },
    "showHours": {
      "type": "boolean",
      "default": true
    },
    "showMinutes": {
      "type": "boolean",
      "default": true
    },
    "showSeconds": {
      "type": "boolean",
      "default": true
    },
    "showSeparator": {
      "type": "boolean",
      "default": true
    },
    "separatorType": {
      "type": "string"
    },
    "ariaLiveType": {
      "type": "string"
    },
    "textColorHover": {
      "type": "string"
    },
    "backgroundColorHover": {
      "type": "string"
    },
    "backgroundGradientHover": {
      "type": "string"
    },
    "width": {
      "type": "string"
    },
    "height": {
      "type": "string",
      "default": "auto"
    },
    "minWidth": {
      "type": "string"
    },
    "minHeight": {
      "type": "string"
    },
    "maxWidth": {
      "type": "string"
    },
    "maxHeight": {
      "type": "string"
    },
    "overflow": {
      "type": "string",
      "default": "visible"
    },
    "dayLabel": {
      "type": "string"
    },
    "daysLabel": {
      "type": "string"
    },
    "hourLabel": {
      "type": "string"
    },
    "hoursLabel": {
      "type": "string"
    },
    "minuteLabel": {
      "type": "string"
    },
    "minutesLabel": {
      "type": "string"
    },
    "secondLabel": {
      "type": "string"
    },
    "secondsLabel": {
      "type": "string"
    }
  }
}